<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="source.labyrinth.controllers.LeaderboardController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0">
         <bottom>
            <Button mnemonicParsing="false" onAction="#returnToMainMenu" text="Back" BorderPane.alignment="CENTER" />
         </bottom>
         <top>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Leaderboard" BorderPane.alignment="CENTER" />
         </top>
         <center>
            <TableView fx:id="tableview" prefHeight="316.0" prefWidth="600.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="id" prefWidth="120.0" text="ID" />
                <TableColumn fx:id="name" prefWidth="120.0" text="Name" />
                  <TableColumn fx:id="total" prefWidth="120.0" text="Total" />
                  <TableColumn fx:id="wins"  prefWidth="120.0" text="Wins" />
                  <TableColumn fx:id="loses" prefWidth="120.0" text="Loses" />
              </columns>
            </TableView>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
